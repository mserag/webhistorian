{
  "manifest_version": 2,
  "name": "LifeLog",
  "version": "1.0",
  "short_name": "Life Log",
  "description": "Upload all my browser history to LifeLog",
  "permissions": [
    "history",
    "tabs",
    "storage",
    "alarms"
  ],
  "browser_action": {
    "default_icon": "images/star48.png"
  },
  "icons": { "16": "images/star16.png",
    "48": "images/star48.png",
    "128": "images/star128.png" },
  "background": {
    "scripts": ["js/app/chrome.js"]
  },
  "offline_enabled": true
}
